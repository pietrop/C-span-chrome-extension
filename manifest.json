{
  "manifest_version": 2,
  "name": "C-Span Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.c-span.org/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
